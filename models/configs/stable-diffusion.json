{
  "base": {
      "pretrained_model_name_or_path": "AstraliteHeart/pony-diffusion",
      "torch_dtype": "float16",
      "safety_checker": null,
      "requires_safety_checker": false,
      "scheduler": {
        "class": "DDIMScheduler",
        "args": {
          "num_train_timesteps": 1000,
          "beta_start": 0.00085,
          "beta_end": 0.012,
          "beta_schedule": "scaled_linear",
          "clip_sample": false,
          "set_alpha_to_one": false,
          "steps_offset": 1
        }
      }
  },
  "use_scheduler": true,
  "enable_model_offload": true
}
